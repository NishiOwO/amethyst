(defun main : i32
    (seq
        (let n2 = (alloca))
        (set (deref n2) =
            (inst Linked
                (v 1)))
        (let n1 =
            (inst Linked
                (v 0)
                (next n2)))
        (deref n1.next).v))

(defstruct Linked
    (v i32)
    (next (* mut Linked)))
